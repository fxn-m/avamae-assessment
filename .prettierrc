{
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "auto",
    "htmlWhitespaceSensitivity": "css",
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "parser": "typescript",
    "printWidth": 160,
    "quoteProps": "consistent",
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "none",
    "useTabs": false,
    "plugins": ["prettier-plugin-tailwindcss"],
    "overrides": [
        {
        "files": "*.json",
        "options": {
            "parser": "json"
        }
        }
    ]
}
